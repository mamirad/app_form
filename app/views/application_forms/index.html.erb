<p id="notice"><%= notice %></p>

<h1>Application Forms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cnic</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Address</th>
      <th>Mobile number</th>
      <th>Apply for</th>
      <th>Highest education</th>
      <th>Subject or areas</th>
      <th>Qualitative research experience</th>
      <th>Upload cv</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_forms.each do |application_form| %>
      <tr>
        <td><%= application_form.name %></td>
        <td><%= application_form.cnic %></td>
        <td><%= application_form.age %></td>
        <td><%= application_form.gender %></td>
        <td><%= application_form.address %></td>
        <td><%= application_form.mobile_number %></td>
        <td><%= application_form.apply_for %></td>
        <td><%= application_form.highest_education %></td>
        <td><%= application_form.subject_or_areas %></td>
        <td>
          <ul>
             <% application_form.qualitative_research_experiences.each do|listt| %>
             <li> <%=listt.name%></li>
         
          <%end%>
          </ul>
         
        </td>
        <td> <%= image_tag  (application_form.upload_cv.url), :size => "260x180" %></td>
        <td><%= link_to 'Show', application_form %></td>
        <td><%#= link_to 'Edit', edit_application_form_path(application_form) %></td>
        <td><%= link_to 'Destroy', application_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application Form', new_application_form_path %>
